{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [],
  "globalDependencies": [
    "tsconfig.json"
  ]
}